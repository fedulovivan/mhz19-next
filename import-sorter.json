{
    "groupRules": [
        "^(react|react-dom|react-redux|react-router|react-router-dom|react-hot-loader/root)$",
        {},
        "^lib",
        "^src",
        "^[.]"
    ],
    "wrappingStyle": {
        "maxBindingNamesPerLine": 2,
        "maxDefaultAndBindingNamesPerLine": 3,
        "maxNamesPerWrappedLine": 1
    },
    "quoteMark": "single",
    "trailingComma": "multiLine",
    "formatExports": false,
    "keepUnused": [".*"],
    "maxLineLength": 0
}
